{
  "name": "fetch.io",
  "version": "5.0.0",
  "description": "extends the whatwg fetch api, makes it easier to use",
  "browser": "build/fetch.js",
  "main": "lib/node.js",
  "scripts": {
    "build": "babel lib --out-dir build",
    "test": "mocha -R spec -t 5000 test/node.js",
    "build-test": "webpack --entry ./test/browser.js --output-filename ./test/browser-build.js",
    "test-cov": "npm run build && istanbul cover node_modules/.bin/_mocha -- -R dot -t 5000 test/*.js",
    "test-browser": "npm run build-test && karma start test/karma.conf.js",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build"
  },
  "repository": "haoxins/fetch.io",
  "keywords": [
    "request",
    "extend",
    "fetch",
    "ajax",
    "api"
  ],
  "files": [
    "build/",
    "lib/"
  ],
  "author": "haoxin",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "istanbul": "^0.4.5",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.2",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-safari-launcher": "^1.0.0",
    "mocha": "^5.1.1",
    "node-libs-browser": "^2.1.0",
    "webpack": "^4.8.3"
  },
  "dependencies": {
    "form-data": "^2.3.2",
    "node-fetch": "^2.1.2"
  }
}
